   <EntryType>Instructive</EntryType>
  </xsd:appinfo>
 </xsd:annotation>
 <xsd:restriction base="xsd:string">
  <xsd:annotation>
   <xsd:documentation>DDMMMYYYY
</xsd:documentation>
   <xsd:appinfo>
    <DataType>String</DataType>
    <Explanation>DDMMMYYYY
</Explanation>
    <MtfRegularExpression>(0[1-9]|(1|2)[0-9]|3(0|1))(JAN|FEB|MAR|APR|MAY|JUN|JUL|AUG|SEP|OCT|NOV|DEC)[0-9]{4}</MtfRegularExpression>
   </xsd:appinfo>
  </xsd:annotation>
  <xsd:length value="9" />
  <xsd:pattern value="(0[1-9]|(1|2)[0-9]|3(0|1))(JAN|FEB|MAR|APR|MAY|JUN|JUL|AUG|SEP|OCT|NOV|DEC)[0-9]{4}" />
 </xsd:restriction>
