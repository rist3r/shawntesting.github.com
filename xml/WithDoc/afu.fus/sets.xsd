       <FieldFormatPositionName>LOCATION UTM 1-METER EASTING</FieldFormatPositionName>
       <FieldFormatPositionNumber>1</FieldFormatPositionNumber>
       <OccurrenceCategory>Mandatory</OccurrenceCategory>
