              <xsd:attributeGroup ref="ism:SecurityAttributesOptionGroup" />
             </xsd:extension>
            </xsd:simpleContent>
           </xsd:complexType>
          </xsd:element>
         </xsd:choice>
         <xsd:attribute name="ffSeq" type="xsd:unsignedShort" fixed="1" />
         <xsd:attributeGroup ref="ism:SecurityAttributesOptionGroup" />
        </xsd:complexType>
       </xsd:element>
      </xsd:sequence>
     </xsd:complexType>
    </xsd:element>
   </xsd:sequence><xsd:attribute name="setid" type="xsd:string" fixed="POLYGON" /></xsd:extension>
