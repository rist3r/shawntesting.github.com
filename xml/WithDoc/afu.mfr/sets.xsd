</xsd:complexType><xsd:complexType name="MessageDowngradingOrDeclassificationDataType">
 <xsd:annotation>
  <xsd:appinfo>
   <SetFormatName>MESSAGE DOWNGRADING OR DECLASSIFICATION DATA</SetFormatName>
   <SetFormatIdentifier>DECL</SetFormatIdentifier>
   <ColumnarIndicator>N</ColumnarIndicator>
   <GroupOfFieldsIndicator>1</GroupOfFieldsIndicator>
   <RepeatabilityForGroupOfFields>1</RepeatabilityForGroupOfFields>
   <SetFormatNote>Field 2 is REQUIRED, if Field 1 uses Alternative B (ORIGINAL CLASSIFICATION AUTHORITY), otherwise Field 2 is PROHIBITED.</SetFormatNote>
   <SetFormatExample>DECL/OCA:USJFCOM/14C/DCDATE:18JUN2025//</SetFormatExample>
   <SetFormatExample>
DECL/OCA:CHIEF D. SMITH, DIV 5, JCS LTR DATED 10 JUN 1998/14A
/DCINST:25X4-IAW APPLICABLE SCG//</SetFormatExample>
   <SetFormatExample>
DECL/OCA:CNO, N7/14A
/DCINST:DECLASSIFY PER INSTRUCTION CONTAINED WITHIN MESSAGE//</SetFormatExample>
   <SetFormatExample>
DECL/DERI:CNO, N7 MESSAGE DTG 210630Z2005/-
/DCINST:DECLASSIFY 3 DAYS BEFORE EVENT//</SetFormatExample>
   <SetFormatExample>
DECL/OCA:ONI, LTR DATED 10 JUN 1998/14C/DCINST:25X1-HUMAN
/DGINST:DOWNGRADE TO (S) 15JUN2030//</SetFormatExample>
   <SetFormatExample>
DECL/OCA:CTF 176 EXORD SHINING SHIELD/14A
/DCINST:UPON THE SUCCESSFUL COMPLETION OF OPERATION SHINING SHIELD
/DGINST:OCA WILL DETERMINE DG UPON REVIEW OF MILESTONE COMPLETIONS//</SetFormatExample>
   <SetFormatExample>
DECL/DERI:CTF 176 EXORD SHINING SHIELD DTG 121330ZNOV2005/-
/DCINST:CONTACT CTF 176 WATCH OFFICER FOR DECLASS INSTRUCTIONS
/DGINST:OCA WILL DETERMINE DG UPON REVIEW OF MILESTONE COMPLETIONS//</SetFormatExample>
   <SetFormatExample>
DECL/OCA:COMNAVSEASYSCOM/14A
/DCINST:STD DRAWING 803-XXXX137 DTD 12NOV1996 AND MARKED X3
/DGINST:CURRENTLY UNDER REVIEW FOR DECLASS OR DWGRADE//</SetFormatExample>
   <SetFormatExample>
DECL/DERI:USJFCOM EXORD SOLID WASTE 98/-/DCINST:AT EXERCISE ENDEX//</SetFormatExample>
   <SetFormatExample>
