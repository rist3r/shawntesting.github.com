    <MtfStructuralRelationshipExplanation>Field 2 in MSGID (Set 3) is assigned the value "MIL-STD-6040(SERIES)".</MtfStructuralRelationshipExplanation>
    <MtfStructuralRelationshipXmlSnRule />
    <MtfStructuralRelationshipXsnRule>if ( not( exists( /*:AcknowledgeMessage/MessageIdentifier/StandardOfMessageTextFormat))) then 'null' else xsn:or3(  string( /*:AcknowledgeMessage/MessageIdentifier/StandardOfMessageTextFormat) eq "MIL-STD-6040(SERIES)")</MtfStructuralRelationshipXsnRule>
   </MtfStructuralRelationship>
   <MtfStructuralRelationship>
    <MtfStructuralRelationshipRule>[3]F3 A /B.1.01.14/</MtfStructuralRelationshipRule>
    <MtfStructuralRelationshipExplanation>Field 3 in MSGID (Set 3) is assigned the value "B.1.01.14".</MtfStructuralRelationshipExplanation>
    <MtfStructuralRelationshipXmlSnRule />
    <MtfStructuralRelationshipXsnRule>if ( not( exists( /*:AcknowledgeMessage/MessageIdentifier/VersionOfMessageTextFormat))) then 'null' else xsn:or3(  string( /*:AcknowledgeMessage/MessageIdentifier/VersionOfMessageTextFormat) eq "B.1.01.14")</MtfStructuralRelationshipXsnRule>
   </MtfStructuralRelationship>
   <MtfStructuralRelationship>
    <MtfStructuralRelationshipRule>[4],AF3 R ([4],AF2 EQ /CHT/ | /DOC/)</MtfStructuralRelationshipRule>
    <MtfStructuralRelationshipExplanation>Field 3 in REF (Set 4) is required if Field 2 in the same REF (Set 4) lexicographically equals "CHT" or "DOC".</MtfStructuralRelationshipExplanation>
    <MtfStructuralRelationshipXmlSnRule />
    <MtfStructuralRelationshipXsnRule>if ( not( exists( /AcknowledgeMessage/Reference))) then 'null' else xsn:and3( for $A in 1 to count( /AcknowledgeMessage/Reference) return xsn:or3(  xsn:not3(   if ( not( exists( /AcknowledgeMessage/Reference[ $A]/CommunicationType/*)) and not( exists( /AcknowledgeMessage/Reference[ $A]/CommunicationType/*/*))) then 'null' else xsn:or3(  string( if ( exists( /AcknowledgeMessage/Reference[ $A]/CommunicationType/*/*)) then string-join( /AcknowledgeMessage/Reference[ $A]/CommunicationType/*/*,"") else string( /AcknowledgeMessage/Reference[ $A]/CommunicationType/*)) eq "CHT",  string( if ( exists( /AcknowledgeMessage/Reference[ $A]/CommunicationType/*/*)) then string-join( /AcknowledgeMessage/Reference[ $A]/CommunicationType/*/*,"") else string( /AcknowledgeMessage/Reference[ $A]/CommunicationType/*)) eq "DOC")),  if ( not( exists( /AcknowledgeMessage/Reference[ $A]))) then 'null' else  count( /AcknowledgeMessage/Reference[ $A]/InformationProduct) != 0  ))</MtfStructuralRelationshipXsnRule>
   </MtfStructuralRelationship>
   <MtfStructuralRelationship>
    <MtfStructuralRelationshipRule>[5],ZFG,ZF3 EQ [4],NF1</MtfStructuralRelationshipRule>
    <MtfStructuralRelationshipExplanation>Field 3 in every Field Group in every ACTAGCY (Set 5) lexicographically must equal Field 1 in at least one occurrence of REF (Set 4).</MtfStructuralRelationshipExplanation>
    <MtfStructuralRelationshipXmlSnRule />
    <MtfStructuralRelationshipXsnRule>if ( not( exists( /*:AcknowledgeMessage/ActionAgencyAndResponse))) then 'null' else xsn:and3( for $expr_1_1 in 1 to count( /*:AcknowledgeMessage/ActionAgencyAndResponse) return if ( not( exists( /*:AcknowledgeMessage/ActionAgencyAndResponse[ $expr_1_1]/GroupOfFields))) then 'null' else xsn:and3( for $expr_1_2 in 1 to count( /*:AcknowledgeMessage/ActionAgencyAndResponse[ $expr_1_1]/GroupOfFields) return if ( not( exists( /*:AcknowledgeMessage/ActionAgencyAndResponse[ $expr_1_1]/GroupOfFields[ $expr_1_2]/SerialLetter))) then 'null' else xsn:or3(  if ( not( exists( /*:AcknowledgeMessage/Reference))) then 'null' else xsn:or3( for $expr1_op1_1 in 1 to count( /*:AcknowledgeMessage/Reference) return if ( not( exists( /*:AcknowledgeMessage/Reference[ $expr1_op1_1]/SerialIdentifier))) then 'null' else string( /*:AcknowledgeMessage/ActionAgencyAndResponse[ $expr_1_1]/GroupOfFields[ $expr_1_2]/SerialLetter) eq string( /*:AcknowledgeMessage/Reference[ $expr1_op1_1]/SerialIdentifier)))))</MtfStructuralRelationshipXsnRule>
   </MtfStructuralRelationship>
   <MtfStructuralRelationship>
    <MtfStructuralRelationshipRule>[7] RP ([3]F10 EQ /CONFIDENTIAL/ | /SECRET/ | /TOP SECRET/ | /RESTRICTED/ | /NATO RESTRICTED/ | /NATO CONFIDENTIAL/ | /NATO SECRET/ | /NATO SECRET-SAVATE/ | /NATO SECRET-AVICULA/ | /COSMIC TOP SECRET/ | /COSMIC TOP SECRET-BOHEMIA/ | /COSMIC TOP SECRET-BALK/ | /COSMIC TOP SECRET ATOMAL/ | /NATO SECRET ATOMAL/ | /NATO CONFIDENTIAL ATOMAL/)</MtfStructuralRelationshipRule>
    <MtfStructuralRelationshipExplanation>DECL (Set 7) is required if Field 10 in MSGID (SET 3) lexicographically equals "CONFIDENTIAL" or "SECRET" or "TOP SECRET" or "RESTRICTED" or "NATO RESTRICTED" or "NATO CONFIDENTIAL" or "NATO SECRET" or "NATO SECRET-SAVATE" or "NATO SECRET-AVICULA" or "COSMIC TOP SECRET" or "COSMIC TOP SECRET-BOHEMIA" or "COSMIC TOP SECRET-BALK" or "COSMIC TOP SECRET ATOMAL" or "NATO SECRET ATOMAL" or "NATO CONFIDENTIAL ATOMAL" otherwise it is prohibited.</MtfStructuralRelationshipExplanation>
    <MtfStructuralRelationshipXmlSnRule />
    <MtfStructuralRelationshipXsnRule>for $cond in  if ( not( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) and not( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*))) then 'null' else xsn:or3(  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "CONFIDENTIAL",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "SECRET",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "TOP SECRET",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "RESTRICTED",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "NATO RESTRICTED",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "NATO CONFIDENTIAL",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "NATO SECRET",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "NATO SECRET-SAVATE",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "NATO SECRET-AVICULA",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "COSMIC TOP SECRET",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "COSMIC TOP SECRET-BOHEMIA",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "COSMIC TOP SECRET-BALK",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "COSMIC TOP SECRET ATOMAL",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "NATO SECRET ATOMAL",  string( if ( exists( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*)) then string-join( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*/*,"") else string( /*:AcknowledgeMessage/MessageIdentifier/MessageSecurityClassification/*)) eq "NATO CONFIDENTIAL ATOMAL")  return if ( $cond instance of xs:string) then 'null' else if ( $cond) then  count( /*:AcknowledgeMessage/MessageDowngradingOrDeclassificationData) != 0  else  count( /*:AcknowledgeMessage/MessageDowngradingOrDeclassificationData) = 0</MtfStructuralRelationshipXsnRule>
   </MtfStructuralRelationship>
   <MtfStructuralRelationship>
    <MtfStructuralRelationshipRule>[7]F2 RP ([7]F1 = FF679-4)</MtfStructuralRelationshipRule>
    <MtfStructuralRelationshipExplanation>Field 2 in DECL (Set 7) is required if Field 1 in DECL (Set 7) uses alternative ORIGINAL CLASSIFICATION AUTHORITY (FF679-4) otherwise it is prohibited.</MtfStructuralRelationshipExplanation>
    <MtfStructuralRelationshipXmlSnRule />
    <MtfStructuralRelationshipXsnRule>for $cond in if ( not( exists( /*:AcknowledgeMessage/MessageDowngradingOrDeclassificationData/SourceForClassification))) then 'null' else xsn:or3(  exists( /*:AcknowledgeMessage/MessageDowngradingOrDeclassificationData/SourceForClassification/OriginalClassificationAuthority))  return if ( $cond instance of xs:string) then 'null' else if ( $cond) then  if ( not( exists( /*:AcknowledgeMessage/MessageDowngradingOrDeclassificationData))) then 'null' else  count( /*:AcknowledgeMessage/MessageDowngradingOrDeclassificationData/ReasonForClassification) != 0  else  if ( not( exists( /*:AcknowledgeMessage/MessageDowngradingOrDeclassificationData))) then 'null' else  count( /*:AcknowledgeMessage/MessageDowngradingOrDeclassificationData/ReasonForClassification) = 0</MtfStructuralRelationshipXsnRule>
