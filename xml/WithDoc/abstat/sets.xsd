           <xsd:attribute name="ffirnFudn" type="xsd:string" fixed="FF593-1" />
           <xsd:attributeGroup ref="ism:SecurityAttributesOptionGroup" />
          </xsd:extension>
         </xsd:complexContent>
        </xsd:complexType>
       </xsd:element>
      </xsd:choice>
      <xsd:attribute name="ffSeq" type="xsd:unsignedShort" fixed="5" />
      <xsd:attributeGroup ref="ism:SecurityAttributesOptionGroup" />
