   <xsd:sequence>
    <xsd:element name="GroupOfFields" minOccurs="1" maxOccurs="unbounded">
     <xsd:complexType>
      <xsd:sequence>
       <xsd:element name="DataEntry" minOccurs="1" maxOccurs="1" nillable="true">
        <xsd:annotation>
         <xsd:documentation>A number associating related lines of data.</xsd:documentation>
         <xsd:appinfo>
          <FieldFormatPositionName>DATA ENTRY</FieldFormatPositionName>
          <FieldFormatPositionNumber>1</FieldFormatPositionNumber>
          <OccurrenceCategory>Mandatory</OccurrenceCategory>
          <FieldFormatPositionConcept>A number associating related lines of data.</FieldFormatPositionConcept>
