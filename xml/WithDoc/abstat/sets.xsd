    <xsd:element name="AirBaseLocationLatLongSeconds" minOccurs="0" maxOccurs="1">
     <xsd:annotation>
      <xsd:documentation>The location of an airbase. Field 7 is used if the airbase is a new airbase or if the base location has not been previously provided.</xsd:documentation>
      <xsd:appinfo>
       <FieldFormatPositionName>AIR BASE LOCATION, LAT/LONG, SECONDS</FieldFormatPositionName>
       <FieldFormatPositionNumber>7</FieldFormatPositionNumber>
       <OccurrenceCategory>Operationally Determined</OccurrenceCategory>
